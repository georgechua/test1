actions: <Widget>[
          
          new Container(
            padding: EdgeInsets.only(right:10.0),
            child: new IconButton(icon: const Icon(Icons.person), iconSize: 35.0, 
            onPressed: () {}))
          ],


//Login Page
new MaterialButton(
                        elevation: 5.0,
                        height: 40.0,
                        minWidth: 100.0,
                        color: Colors.blue,
                        textColor: Colors.white,
                        child: new Text("Sign In", style: new TextStyle(fontWeight: FontWeight.bold, fontSize: 17.0),),
                        
                        onPressed: () => _signIn().then((FirebaseUser user) => Navigator.of(context).pushAndRemoveUntil(new MaterialPageRoute(builder: (BuildContext context) => MyHomePage()), (Route route) => route == null)).catchError((e) => print(e)),
                      
                        splashColor: Colors.lightBlue[900],
                    ), 
 new MaterialButton(
                        elevation: 5.0,
                        height: 40.0,
                        minWidth: 100.0,
                        color: Colors.red,
                        textColor: Colors.white,
                        child: new Text("Sign Out", style: new TextStyle(fontWeight: FontWeight.bold, fontSize: 17.0),),
                        
                        onPressed: () => _signOut(),
                      
                        splashColor: Colors.lightBlue[900],
                    ),